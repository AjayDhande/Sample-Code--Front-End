<head>
  <meta name="viewport" content="width=device-width" />
  <% if current_subdomain.is_ocm? %>
    <%= render 'favicon' %>
    <title>Online Church Media - Media Store</title>
  <% else %>
    <% favicon = Theme.find_by_subdomain_id(current_subdomain.id) %>
    <% if favicon.present? && favicon.favicon_image_path.present?%>
      <link href="/assets/images/<%=favicon.favicon_image_path%>" rel="shortcut icon" type="image/x-icon"/>
    <%else%>
      <link href="<%= asset_path 'subdomain_favicon.png'%>" rel="shortcut icon" type="image/x-icon"/>
    <% end %>
      <% churchs_name = User.where(:username => current_subdomain.subdomain_name).first.church_name %>
      <title><%= churchs_name.titlecase %> - Media Store</title>
  <% end %>

  <meta charset="utf-8">
  <%if @media_details != true %>
    <meta property="og:title" content="Online Church Media - Your Online Church Media Store"/>
    <meta property="og:description" content="Click here to stream and download the latest media from here." />
    <meta property="og:url" content="<%=request.url%>"/>
    <meta property="og:image" content="<%= request.base_url %><%= @logo_path rescue nil%>"/>
  <%else%>
    <meta property="og:title" content="<%= File.basename(@uploaded_media.media_file_name,File.extname(@uploaded_media.media_file_name)).gsub("_", " ") rescue nil%>"/>
    <meta property="og:description" content="<%= @uploaded_media.presenter_name rescue nil%>" />
    <meta property="og:type" content="music.song"/>
    <meta property="og:url" content="<%=request.url%>"/>
    <meta property="og:image" content="<%= request.base_url %><%= @uploaded_media.media_art.url(:thumb) rescue nil %>"/>
  <%end%>
  <meta property="og:site_name" content="onlinechurchmedia"/>
  <meta property="fb:app_id" content="733241933481400" />
  <meta property="og:image:width" content="600" />
  <meta property="og:image:height" content="315" />
  <meta http-equiv="X-UA-Compatible" content="IE=10; IE=9; IE=8; IE=7; IE=EDGE" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1">
  <%= include_gon %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
